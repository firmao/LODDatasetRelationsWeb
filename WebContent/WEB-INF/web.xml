<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>DatasetMatchingWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Matching datasets</description>
    <display-name>MatchingServlet</display-name>
    <servlet-name>MatchingServlet</servlet-name>
    <servlet-class>web.servlet.matching.MatchingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MatchingServlet</servlet-name>
    <url-pattern>/matchDataset</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Upload HDT files</description>
    <display-name>UploadHDTfile</display-name>
    <servlet-name>UploadHDTfile</servlet-name>
    <servlet-class>web.servlet.matching.UploadHDTfile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadHDTfile</servlet-name>
    <url-pattern>/UploadHDTfile</url-pattern>
  </servlet-mapping>
</web-app>